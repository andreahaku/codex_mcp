{
  "name": "@codex/mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Codex CLI integration with Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "codex-mcp": "dist/index.js"
  },
  "scripts": {
    "start": "./start.sh",
    "start:local": "./start.sh",
    "install:setup": "./install.sh",
    "build": "tsc && chmod +x dist/*.js",
    "dev": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "mcp",
    "codex",
    "openai",
    "claude",
    "ai",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "dotenv": "^16.4.5",
    "winston": "^3.14.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.2"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.17.1"
}
